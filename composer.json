{
    "name": "thefair/hyperf-rocketmq",
    "type": "library",
    "description": "The PHP library for hyperf rocketmq",
    "keywords": [
        "rocketmq"
    ],
    "homepage": "https://github.com/thefair-net/hyperf_rocketmq",
    "license": "MIT",
    "authors": [
        {
            "name": "Zhang Han",
            "email": "zhanghan@thefair.net.cn"
        },
        {
            "name": "Liu mingzhi",
            "email": "liumingzhi@thefair.net.cn"
        }
    ],
    "require": {
        "php": ">=7.3",
        "hyperf/guzzle": "~2.1.0",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "TheFairLib\\RocketMQ\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        },
        "hyperf": {
            "config": "TheFairLib\\RocketMQ\\ConfigProvider"
        }
    }
}
